
<!DOCTYPE html>
<html>
<head>
  <title>Homework</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= link_to 'Home', new_customer_path,class: 'btn btn-default' %>
  <%if current_admin.nil? %>
  <%= link_to 'Admin login', new_admin_session_path, class: 'btn btn-default' %>
  <% else %>
<%= link_to 'logout', destroy_admin_session_path ,class: 'btn btn-default', method: :delete%>
<%= link_to 'Bookings', bookings_path ,class: 'btn btn-default'%>
<%= link_to 'Customers', customers_path,class: 'btn btn-default' %>
<%= link_to 'Cleaners', cleaners_path ,class: 'btn btn-default'%>
<%= link_to 'City', cities_path ,class: 'btn btn-default'%>

<hr>
<% end %>
<%= yield %>
<% if flash[:notice] %>
    <%= flash[:notice] %></div>
  <% end %>
</body>
</html>
