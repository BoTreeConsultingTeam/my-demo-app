<p id="notice"><%= notice %></p>
<h3>Profile</h3>
<br>
<br>
<p>
  <strong>First name:</strong>
  <%= @cleaner.first_name %>
</p>
<p>
  <strong>Last name:</strong>
  <%= @cleaner.last_name %>
</p>
<p>
  <strong>Quality score:</strong>
  <%= @cleaner.quality_score %>
</p>
<p>
  <strong>Cities:</strong>
  <% @cleaner.cities.each do |city| %>
    <%= city.name %>&nbsp&nbsp
  <% end %>
</p>
<%= link_to 'Edit', edit_cleaner_path(@cleaner), class: "btn btn-default btn-sm" %>&nbsp&nbsp
<%= link_to 'Back', cleaners_path, class: "btn btn-default btn-sm" %>
