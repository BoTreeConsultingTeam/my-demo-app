<p id="notice"><%= notice %></p>

<p>
  <strong>First name:</strong>
  <%= @cleaner.first_name %>
</p>

<p>
  <strong>Last name:</strong>
  <%= @cleaner.last_name %>
</p>

<p>
  <strong>Quality score:</strong>
  <%= @cleaner.quality_score %>
</p>

<p>
  <strong>Email:</strong>
  <%= @cleaner.email %>
</p>

<p>
  <strong>city:</strong>
  <%= @cleaner.cities.pluck(:name).join(',') %>
</p>
<%= link_to edit_cleaner_path(@cleaner),title: 'Edit Cleaner' do %>
  <span class="glyphicon glyphicon-pencil"></span>
<% end %>|
<%= link_to  cleaners_path,title: 'Back All Cleaner' do %>
  <span class="glyphicon glyphicon-arrow-left"></span>
<% end %>
