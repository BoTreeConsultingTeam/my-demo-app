<p id="notice"><%= notice %></p>

<h1>Listing Citys</h1>

<table border="2" borderColor="classic" bgcolor="clean" >
  <thead>
    <tr>
      <th>City Name</th>
      <th colspan="3">Action</th>
    </tr>
  </thead>

  <tbody>
    <% @citys.each do |city| %>
      <tr>
        <td><%= city.city %></td>
        <td><%= link_to 'Show', city %></strong></td>
        <td><%= link_to 'Edit', edit_city_path(city) %></td>
        <td><%= link_to 'Destroy', city method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br/>
<%= link_to 'New City', new_city_path %>
